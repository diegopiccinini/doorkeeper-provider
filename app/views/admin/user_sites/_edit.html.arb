h2 user.name
render partial: 'site_filter'
h3 link_to 'Back to Admin', admin_user_sites_path
  table_for sites, class: 'tags-table' do
     column :url
     column :enabled
     column 'Action' do |site|
       if user.has_site? site
         span 'Remove'
       else
         span 'Add'
       end
     end

  end
